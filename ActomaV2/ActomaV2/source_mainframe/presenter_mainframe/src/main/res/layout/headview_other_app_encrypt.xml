<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/base_white_95"
    android:id="@+id/head_view_other_app_encrypt_listLayout2"
    tools:context="com.xdja.presenter_mainframe.ui.ViewNewEncrypt">
    <!--android:background="@color/listview_bg_withgroup"-->
    <!--tools:ignore="Overdraw"-->

    <!--<LinearLayout-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:orientation="vertical"-->
        <!--tools:ignore="UselessParent">-->


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                tools:ignore="UseCompoundDrawables">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/base_16"
                    android:contentDescription="@null"
                    android:src="@drawable/af_setting_img_logo"
                    tools:ignore="RtlHardcoded" />

                <TextView
                    android:id="@+id/head_other_app_encrypt_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/base_16"
                    android:layout_marginLeft="@dimen/base_16"
                    android:layout_marginRight="@dimen/base_16"
                    android:layout_marginTop="@dimen/base_16"
                    android:background="@color/base_white_95"
                    android:lineSpacingMultiplier="1.1"
                    android:text="@string/other_app_encrypt_msg8"
                    android:textColor="@color/encrypt_title_black"
                    android:textSize="13sp" />
            </LinearLayout>

            <View
                style="@style/view_line_style" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/base_32"
                android:background="@color/listview_bg_nogroup"
                android:gravity="center_vertical"
                android:paddingStart="@dimen/base_16"
                android:text="@string/other_app_encrypt_msg1"
                android:textColor="@color/base_black_65"
                android:textSize="@dimen/body1_14"
                tools:ignore="RtlSymmetry" />

            <View
                style="@style/view_line_style" />
    <!--</LinearLayout>-->
</LinearLayout>
