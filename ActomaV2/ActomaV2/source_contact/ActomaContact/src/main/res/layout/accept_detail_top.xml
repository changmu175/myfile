<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="wrap_content"    tools:ignore="MergeRootFrame"    >        <LinearLayout            android:id="@+id/contact_detail_title_layout"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical"            android:background="@drawable/detail_background">            <RelativeLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_horizontal"                android:layout_marginTop="68dp"                android:background="@drawable/img_bg_friend_circle">                <com.xdja.comm.circleimageview.CircleImageView                    android:id="@+id/contact_detail_avater"                    android:layout_width="@dimen/contact_detail_circle_img_width"                    android:layout_height="@dimen/contact_detail_circle_img_height"                    android:layout_centerHorizontal="true"                    android:layout_centerVertical="true"                    android:src="@drawable/img_avater_100" />            </RelativeLayout>            <TextView                android:id="@+id/contact_person_name"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_horizontal"                android:layout_marginTop="@dimen/base_16"                android:layout_marginLeft="25dp"                android:layout_marginRight="25dp"                android:textColor="@color/detail_font_color"                android:textSize="@dimen/head_16" />            <LinearLayout                android:id="@+id/name_lay"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_horizontal"                android:layout_marginTop="@dimen/base_8"                android:gravity="center_horizontal"                android:orientation="horizontal"                android:visibility="gone">                <TextView                    android:id="@+id/name_label"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="@string/contact_detail_name"                    android:textColor="@color/detail_font_color"                    android:textSize="@dimen/body1_14" />                <TextView                    android:id="@+id/name"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:paddingLeft="4dp"                    android:textColor="@color/detail_font_color"                    android:textSize="@dimen/body1_14"/>            </LinearLayout>            <LinearLayout                android:id="@+id/ninkname_lay"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_horizontal"                android:layout_marginTop="@dimen/base_8"                android:orientation="horizontal"                android:visibility="gone">                <TextView                    android:id="@+id/ninkname_label"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="@string/nickname"                    android:textColor="@color/detail_font_color"                    android:textSize="@dimen/body1_14" />                <TextView                    android:id="@+id/nickname"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:paddingLeft="4dp"                    android:textColor="@color/detail_font_color"                    android:textSize="@dimen/body1_14"/>            </LinearLayout>            <LinearLayout                android:id="@+id/account_number_lay"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_horizontal"                android:layout_marginTop="@dimen/base_8"                android:orientation="horizontal">                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:text="@string/account"                    android:textColor="@color/detail_font_color"                    android:textSize="@dimen/body1_14" />                <TextView                    android:id="@+id/account_number"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:paddingLeft="4dp"                    android:textColor="@color/detail_font_color"                    android:textSize="@dimen/body1_14"/>            </LinearLayout>        </LinearLayout>    <include        android:id="@+id/toolbar"        layout="@layout/view_toolbar" /></FrameLayout>