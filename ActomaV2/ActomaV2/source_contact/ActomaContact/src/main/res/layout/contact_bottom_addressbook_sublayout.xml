<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    tools:ignore="Overdraw"    >    <!--overdraw-->    <!--android:background="@color/base_gray_f4"-->    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="@dimen/base_32">        <TextView            android:id="@+id/address_book_text"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="@dimen/base_16"            android:text="@string/contact_friend_top_item_text"            android:textColor="@color/base_black_65"            android:textSize="@dimen/body1_14" />        <View            android:layout_width="match_parent"            android:layout_height="@dimen/list_divider_height"            android:layout_alignParentBottom="true"            android:background="@color/base_horizontal_line" />    </RelativeLayout>    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="@dimen/base_48"        android:background="@color/base_white_100">        <TextView            android:id="@+id/address_book_name_colon"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="@dimen/base_16"            android:text="@string/contact_layout_name"            android:textColor="@color/base_black_65"            android:textSize="@dimen/menu_14" />        <TextView            android:id="@+id/address_book_name_text"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="82dp"            android:textColor="@color/base_black_95"            android:textSize="@dimen/head_16" />        <View            android:layout_width="match_parent"            android:layout_height="@dimen/list_divider_height"            android:layout_alignParentBottom="true"            android:background="@color/base_horizontal_line" />    </RelativeLayout>    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="48dp"        android:background="@color/base_white_100"        >        <TextView            android:id="@+id/address_book_department_colon"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="@dimen/base_16"            android:text="@string/contact_layout_partment"            android:textColor="@color/base_black_65"            android:textSize="@dimen/menu_14" />        <TextView            android:id="@+id/address_book_department_text"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_centerVertical="true"            android:layout_marginLeft="82dp"            android:textColor="@color/base_black_95"            android:textSize="@dimen/head_16" />        <View            android:layout_width="match_parent"            android:layout_height="@dimen/list_divider_height"            android:layout_alignParentBottom="true"            android:background="@color/base_horizontal_line" />    </RelativeLayout>    <include android:id="@+id/phone_layout_1" layout="@layout/company_phone_item" />  