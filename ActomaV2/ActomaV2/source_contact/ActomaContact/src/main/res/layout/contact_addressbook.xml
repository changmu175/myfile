<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/img_voip_call"    tools:ignore="Overdraw"    >    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent"        android:fillViewport="true">        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="wrap_content">            <LinearLayout                android:id="@+id/contact_detail_title_layout"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical">                <RelativeLayout                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center_horizontal"                    android:layout_marginTop="68dp"                    android:background="@drawable/img_bg_friend_circle">                    <com.xdja.comm.circleimageview.CircleImageView                        android:id="@+id/contact_detail_avater"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_centerHorizontal="true"                        android:layout_centerVertical="true"                        android:src="@drawable/at_teem_100" />                </RelativeLayout>                <TextView                    android:id="@+id/contact_person_name"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center_horizontal"                    android:layout_marginTop="@dimen/base_16"                    android:textColor="@color/detail_font_color"                    android:textSize="@dimen/head_16"                    android:layout_marginBottom="40dp"/>            </LinearLayout>            <LinearLayout                android:id="@+id/account_bottom_layout"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_below="@id/contact_detail_title_layout"                android:background="@color/base_gray_f4"                android:orientation="vertical">                <LinearLayout                    android:id="@+id/call_phone_layout"                    android:layout_width="match_parent"                    android:layout_height="@dimen/base_56"                    android:background="@drawable/bg_list_item"                    android:clickable="true"                    android:descendantFocusability="blocksDescendants"                    android:orientation="horizontal"                    android:gravity="center"                    >                    <TextView                        android:id="@+id/address_book_phone_number_colon"                        android:layout_width="0dp"                        android:layout_height="match_parent"                        android:layout_weight="2.2"                        android:layout_marginLeft="@dimen/base_16"                        android:text="@string/contact_customer_phone"                        android:textColor="#F3000000"                        android:textSize="@dimen/head_16"                        android:gravity="center_vertical|left"                       />                    <TextView                        android:id="@+id/phone_number_text_1"                        android:layout_width="0dp"                        android:layout_height="match_parent"                        android:text="@string/actoma_phone_number"                        android:layout_weight="5"                        android:textColor="#F3000000"                        android:textSize="@dimen/head_16"                        android:gravity="center_vertical|left"                        />                    <ImageButton                        android:id="@+id/phone"                        android:layout_width="wrap_content"                        android:layout_height="match_parent"                        android:layout_weight="1"                        android:layout_marginRight="@dimen/base_16"                        android:background="@null"                        android:src="@drawable/btn_call_selector"                        android:contentDescription="@null"/>                 </LinearLayout>                <View                    android:layout_width="match_parent"                    android:layout_height="@dimen/list_divider_height"                    android:background="@color/base_horizontal_line" />                <RelativeLayout                    android:id="@+id/come_in"                    android:layout_height="@dimen/contact_friend_includ_item_heght"                    android:layout_width="match_parent"                    android:layout_marginTop="@dimen/base_20"                    android:layout_marginLeft="@dimen/contact_friend_include_item_margin"                    android:layout_marginRight="@dimen/contact_friend_include_item_margin"                    android:layout_gravity="center"                    android:gravity="center"                    android:clickable="true"                    android:background="@drawable/bg_common_btn_seletor">                    <TextView                        android:id="@+id/open_encription_serve_text"                        android:layout_height="match_parent"                        android:layout_width="wrap_content"                        android:text="@string/contact_address_btn_text"                        android:textColor="@color/base_black_95"                        android:textSize="@dimen/menu_14"                        android:gravity="center"                        />                </RelativeLayout>            </LinearLayout>        </RelativeLayout>    </ScrollView>    <include        android:id="@+id/toolbar"        layout="@layout/view_toolbar" /></FrameLayout>